                <!-- JavaScript code to render the pie chart -->
                <script>
                    // Data for the pie chart
                    const pieChartData2 = [
                        { label: 'Borrowing', value: 16.5 },
                        { label: 'RFR', value: 16.5 },
                        { label: 'Credit Rating', value: 16.5 },
                        { label: 'Prime Reference Rate', value: 16.5 },
                        { label: 'COB', value: 16.5 },
                        { label: 'T Bill', value: 16.5 }
                    ];

                    // Extract labels and data from the data array
                    const labels2 = pieChartData2.map(item => item.label);
                    const data2 = pieChartData2.map(item => item.value);

                    // Render the pie chart using Chart.js library
                    const ctx2 = document.getElementById('myPieChart2').getContext('2d');
                    const myPieChart2 = new Chart(ctx2, {
                        type: 'doughnut',
                        data: {
                            labels: labels2,
                            datasets: [{
                                data: data2,
                                backgroundColor: [
                                    '#D76A66',//'rgba(0, 185, 255, 0.99)',   //  Blue
                                    '#72B9f0',//'rgba(255, 108, 22, 0.99)', // Orange
                                    '#744FE6',//'rgba(86, 217, 22, 1)',   //Green
                                    '#E1A34D',//'rgba(255, 47, 92, 1)',   //red
