                                    '#6fC884'//'rgba(210, 118, 246, 0.96)'    // purple
                                ]

                            }]
                        },
                        options: {
                            plugins: {
                                chartJsPlugin3d: {
                                    enabled: true,
                                    alpha: 85,
                                    beta: 50
                                },
                                legend: {
                                    position: 'bottom',
                                    align: 'center',
                                    labels: {
                                        color: 'black',
                                        boxWidth: 40,
                                        padding: 20
                                    }
                                },
                                tooltip: {
                                    backgroundColor: 'rgba(0, 0, 0, 0.8)',
                                    displayColors: false
                                }
                            },
                            cutout: '60%',
                            responsive: false,
                            height: 400,
                            width: 550,
                            onClick: (event, elements) => {
                                if (elements && elements.length > 0) {
                                    const index = elements[0].index;
                                    const label = labels2[index];
                                    if (label == 'T Bill') {
                                        move2('T_Bill');
                                    }
                                    else if (label == 'Prime Reference Rate') {
                                        move2('Prime_Ref_Rate');
                                    } else if (label == 'Credit Rating') {
                                        move2('Credit_Rating');
                                    }
                                    else {
                                        move2(label);
                                    }

                                }
                            }
                        }
                    });

                </script>
